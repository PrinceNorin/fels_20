<h1>Sign In</h1>

<%= form_tag sessions_path do %>
  <div><%= label_tag :username %>
  <%= text_field_tag :username, "", autofocus: true %></div>
  
  <div><%= label_tag :password %>
  <%= password_field_tag :password %></div>
  
  <div><%= submit_tag "Sign In", class: "btn btn-primary" %></div>
<% end %>